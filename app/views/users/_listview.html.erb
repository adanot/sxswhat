<% provide(:title, 'Events') %>
<h1>Schedule</h1>
      <table class='table'>
    <tr>
      <th>Event Name</th>
      <th>Location</th>
      <th>Date</th>
      <th>Time</th>
    </tr>
  <% @events.each do |event| %>
          <tr>
            <td><%= link_to event.name, event %></td>
            <td><%= event.location %></td>
     	    <td><%= event.date.strftime("%m-%d-%Y") %></td>
            <td><%= event.tstart.strftime('%l:%M %p') %></td>
          </tr>
  <% end %>

      </table>
